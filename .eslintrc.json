{
    "env": {
        "browser": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parser": "babel-eslint",
    "plugins": [
        "prettier"
    ],
    "rules": {
        "import/prefer-default-export": "warn",
        "no-param-reassign": "warn",
        "no-plusplus": "off",
        "prettier/prettier": "error"
    }
}
